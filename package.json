{
  "name": "odin",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "package": "dotenv -- sls package",
    "deploy": "dotenv -- sls deploy",
    "remove": "dotenv -- sls remove",
    "postremove": "rimraf infrastructure/stack-outputs.yml",
    "test": "jest --coverage",
    "watch-tests": "jest --watch",
    "codecov": "codecov -f coverage/*.json",
    "validate-circleci": "circleci config validate",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "project-stats": "cloc . --quiet --exclude-dir=node_modules,.serverless,diagrams,coverage,package-lock.json,.gitignore,LICENSE,.env"
  },
  "dependencies": {
    "@manwaring/lambda-wrapper": "^3.6.3",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@serverless/event-mocks": "^1.1.1",
    "@types/aws-lambda": "^8.10.57",
    "@types/aws-sdk": "^2.7.0",
    "@types/chance": "^1.1.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "aws-sdk": "^2.709.0",
    "chance": "^1.1.6",
    "cloc": "^2.5.1",
    "codecov": "^3.7.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^5.0.6",
    "git-rev-sync": "^2.0.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.74.1",
    "serverless-cloudformation-resource-counter": "^1.0.3",
    "serverless-prune-versions": "^0.1.14",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.3.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test && npm run validate-circleci && npm run project-stats"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "author": "Made with â™¥ by Phillip Manwaring"
}
